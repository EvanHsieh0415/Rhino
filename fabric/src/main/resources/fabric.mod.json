{
	"schemaVersion": 1,
	"id": "rhino",
	"version": "${version}",
	"name": "Rhino",
	"description": "A fork of Mozilla's Rhino library, modified for use in mods",
	"authors": [
		"LatvianModder",
		"Mozilla"
	],
	"contact": {
		"sources": "https://github.com/KubeJS-Mods/Rhino",
		"issues": "https://github.com/KubeJS-Mods/Rhino/issues"
	},
	"license": "MPL-2.0",
	"environment": "*",
	"entrypoints": {
		"main": [
			"dev.latvian.mods.rhino.mod.fabric.RhinoModFabric"
		]
	},
	"mixins": [
		"rhino-common.mixins.json"
	],
	"depends": {
	},
	"breaks": {
		"quilt_loader": "<0.17.0"
	},
	"custom": {
		"modmenu:api": true
	},
	"accessWidener": "rhino.accesswidener"
}
