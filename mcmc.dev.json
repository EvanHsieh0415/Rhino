{
	"type": "architectury",
	"java": "17",
	"release": {
		"branch": "1.18/main"
	},
	"dev": {
		"branch": "1.18/dev"
	},
	"env": {
		"build_number": "GITHUB_RUN_NUMBER"
	},
	"artifacts": [
		{
			"suffix": "common",
			"directory": "common/build/libs",
			"pattern": "rhino-([\\d-.]+build\\.\\d+)\\.jar"
		},
		{
			"suffix": "common",
			"directory": "common/build/libs",
			"pattern": "rhino-([\\d-.]+build\\.\\d+)-sources\\.jar",
			"classifier": "sources"
		},
		{
			"suffix": "forge",
			"directory": "forge/build/libs",
			"pattern": "rhino-forge-([\\d-.]+build\\.\\d+)\\.jar"
		},
		{
			"suffix": "fabric",
			"directory": "fabric/build/libs",
			"pattern": "rhino-fabric-([\\d-.]+build\\.\\d+)\\.jar"
		}
	]
}